{
  "name": "robot-admin",
  "version": "1.0.1",
  "private": true,
  "description": "Robot 中后台管理系统团队架构模板 - 基于 Vue3 + Vite 的企业级中后台解决方案",
  "author": {
    "name": "ChenY",
    "email": "ycyplus@gmail.com",
    "url": "https://github.com/ChenyCHENYU"
  },
  "license": "MIT",
  "scripts": {
    "dev": "env-manager dev && vite && vite",
    "build": "env-manager prod && vite build",
    "build:test": "env-manager test && vite build --mode test --emptyOutDir",
    "build:staging": "env-manager staging && vite build --mode staging --profile",
    "preview": "bun run build && vite preview",
    "test:unit": "vitest",
    "type-watch": "vue-tsc --noEmit --watch",
    "type-build": "vue-tsc --build tsconfig/tsconfig.app.json --force",
    "build:check": "bun run type-build && bun run build",
    "lint": "oxlint . --fix -D correctness --ignore-path .gitignore && eslint . --fix",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "commit": "bunx cz",
    "check-branch": "gbd",
    "check-type": "ttc check --format",
    "deploy": "bash ./scripts/merge-and-deploy.sh",
    "sync-deploy-branch": "bash ./scripts/sync-dev.sh"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/list": "^6.1.17",
    "@fullcalendar/vue3": "^6.1.17",
    "@iconify-json/ri": "^1.2.5",
    "@iconify/vue": "^5.0.0",
    "@kangc/v-md-editor": "^2.3.18",
    "@splinetool/runtime": "^1.9.92",
    "@tato30/vue-pdf": "^1.11.4",
    "@vercel/analytics": "^1.5.0",
    "@visactor/vtable-gantt": "^1.19.3",
    "@vue-flow/core": "1.45.0",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.9.0",
    "driver.js": "^1.3.6",
    "echarts": "^5.6.0",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "husky": "7.0.1",
    "jszip": "^3.10.1",
    "jszip-utils": "^0.1.0",
    "mammoth": "^1.9.1",
    "motion-v": "^1.1.0-alpha.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "print-js": "^1.6.0",
    "robot-admin-env-manager": "^1.0.1",
    "ts-type-cleaner": "^1.1.3",
    "vue": "^3.5.13",
    "vue-command-palette": "^0.2.3",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.0",
    "vue3-puzzle-vcode": "^1.1.7",
    "wangeditor": "^4.7.15",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/la": "^1.2.1",
    "@iconify-json/mdi": "^1.2.3",
    "@inspira-ui/plugins": "^0.0.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/runtime-core": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "^9.21.0",
    "eslint-plugin-jsdoc": "^50.6.11",
    "eslint-plugin-oxlint": "^0.15.13",
    "eslint-plugin-vue": "~10.0.0",
    "git-branch-check-diff-commits": "^1.1.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "naive-ui": "^2.41.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.13",
    "prettier": "3.5.3",
    "sass": "^1.87.0",
    "typescript": "~5.8.0",
    "unocss": "66.0.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.2.1",
    "vite-console-plugin": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix --no-cache",
      "prettier --write"
    ]
  }
}