{
  "name": "bun_vite_uno_naive",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "bun run type-build && bun run build-prod",
    "build-prod": "vite build --mode production",
    "build:test": "vite build --mode test --emptyOutDir",
    "build:staging": "vite build --mode staging --profile",
    "preview": "bun run build-prod && vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --watch",
    "type-build": "vue-tsc --build tsconfig.app.json --force",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "commit": "bunx cz"
  },
  "dependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "husky": "7.0.1",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@unocss/eslint-config": "66.0.0",
    "@unocss/preset-attributify": "66.0.0",
    "@unocss/preset-uno": "66.0.0",
    "@unocss/vite": "66.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/runtime-core": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "^9.21.0",
    "eslint-plugin-oxlint": "^0.15.13",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "naive-ui": "^2.41.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.13",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "unocss": "66.0.0",
    "unocss-cli": "^0.3.1",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix --no-cache",
      "prettier --write"
    ]
  }
}
